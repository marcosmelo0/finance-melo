{
  "expo": {
    "name": "Finance Melo",
    "slug": "Finance-Melo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/splash_logo.png",
    "scheme": "finance-melo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/splash_logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#0c0c1b"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marcosmelo0.financemelo",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/splash_logo.png",
        "backgroundColor": "#0c0c1b"
      },
      "package": "com.marcosmelo0.financemelo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d46dd851-d7ec-4071-a145-ab0a97e81b3b"
      }
    }
  }
}
